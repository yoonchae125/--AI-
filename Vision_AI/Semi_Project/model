    "imagePath = 'img'\n",
    "categories = [\"cheyoung\",\"dahyeon\",\"jeongyeon\",\"jihyo\",\"mina\",\"momo\",\"sana\", \"nayeon\",\"tzuyu\"]\n",
    "nb_classes = len(categories)"
   ]
  },
  {
